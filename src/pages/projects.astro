<style>
  p {
    margin: 0;
  }

  .hashtags {
    display: flex;
    gap: 0.75rem;
  }

  .icon {
    height: 2rem;
    width: 2rem;
  }

  .icon svg {
    height: 2rem;
    width: auto;
    text-decoration: inherit;
    color: #ffffff;
  }

  .icon svg:is(:hover, :focus-within) {
    color: #c6ae59;
  }
</style>
